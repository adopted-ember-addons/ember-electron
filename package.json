{
  "name": "ember-electron",
  "version": "5.0.0",
  "description": "An addon for building Desktop apps with Ember and Electron.",
  "keywords": [
    "atom",
    "chromium",
    "electron",
    "ember-addon"
  ],
  "homepage": "https://ember-electron.js.org",
  "bugs": {
    "url": "https://github.com/adopted-ember-addons/ember-electron/issues"
  },
  "repository": "https://github.com/adopted-ember-addons/ember-electron.git",
  "license": "MIT",
  "author": {
    "name": "Felix Rieseberg",
    "email": "felix@felixrieseberg.com",
    "url": "http://www.felixrieseberg.com"
  },
  "contributors": [
    {
      "name": "Aidan Nulman",
      "email": "aidan@isleofcode.com",
      "url": "https://isleofcode.com"
    },
    {
      "name": "Florian Pichler",
      "url": "http://ylk.gd/"
    },
    {
      "name": "Ben Demboski",
      "email": "ben@turbopatent.com",
      "url": "https://github.com/bendemboski"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "release": "release-it",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:electron": "yarn test-fast && yarn test-slow",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test-fast": "mocha node-tests/integration/**/*.js node-tests/unit/**/*.js",
    "test-slow": "mocha node-tests/acceptance/**/*.js"
  },
  "dependencies": {
    "@electron-forge/core": "^6.0.5",
    "chalk": "^4.1.2",
    "debug": "^4.1.1",
    "ember-cli-babel": "^7.26.11",
    "execa": "^5.0.0",
    "find-yarn-workspace-root": "^2.0.0",
    "ncp": "^2.0.0",
    "portfinder": "^1.0.25",
    "rimraf": "^3.0.2",
    "silent-error": "^1.1.1",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.1.0",
    "@embroider/test-setup": "^3.0.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^8.2.0",
    "console-ui": "^3.1.1",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~5.1.0",
    "ember-cli-addon-docs": "^5.0.0",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-clean-css": "^2.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^3.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "^4.0.0",
    "ember-fetch": "^8.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^10.1.1",
    "ember-source": "~5.1.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.9.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^8.0.0",
    "fs-extra": "^11.0.0",
    "loader.js": "^4.7.0",
    "mocha": "^10.1.0",
    "prettier": "^2.8.8",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.5.0",
    "release-it-lerna-changelog": "^5.0.0",
    "sass": "^1.55.0",
    "sinon": "^15.0.1",
    "sinon-chai": "^3.7.0",
    "stylelint": "^15.9.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "stylelint-prettier": "^3.0.0",
    "tmp": "^0.2.1",
    "webpack": "^5.88.1"
  },
  "peerDependencies": {
    "ember-cli": ">= 3.4.0",
    "ember-cli-dependency-checker": ">= 3.1.0",
    "ember-source": ">= 4.0.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "defaultBlueprint": "ember-electron-postinstall",
    "after": [
      "ember-cli-sri",
      "ember-engines"
    ]
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  },
  "volta": {
    "node": "16.20.1",
    "yarn": "1.22.19"
  }
}
