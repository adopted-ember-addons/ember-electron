    "rsvp": "^3.2.1",
    "socket.io": "^1.4.8",
    "symlink-or-copy": "^1.1.8",
    "testem": "^1.15.0",
    "tree-kill": "^1.1.0",
    "loader.js": "^4.2.3"
  },
  "ember-addon": {
    "defaultBlueprint": "ember-electron",
    "versionCompatibility": {
      "ember": ">=2.3.0"
    },
    "after": [
      "ember-cli-sri"
    ]
  },
  "config": {
    "forge": "./ember-electron/electron-forge-config.js"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "lib/resources/**/*.js",
      "lib/blueprints/**/*.js"
    ]
  }
