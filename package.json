{
  "name": "ember-electron",
  "version": "2.10.0",
  "description": "An addon for building Desktop apps with Ember and Electron.",
  "keywords": [
    "ember-addon",
    "electron",
    "atom",
    "chromium"
  ],
  "repository": "https://github.com/adopted-ember-addons/ember-electron.git",
  "license": "MIT",
  "author": {
    "name": "Felix Rieseberg",
    "email": "felix@felixrieseberg.com",
    "url": "http://www.felixrieseberg.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "contributors": [
    {
      "name": "Aidan Nulman",
      "email": "aidan@isleofcode.com",
      "url": "https://isleofcode.com"
    },
    {
      "name": "Florian Pichler",
      "url": "http://ylk.gd/"
    },
    {
      "name": "Ben Demboski",
      "email": "ben@turbopatent.com",
      "url": "https://github.com/bendemboski"
    }
  ],
  "bugs": {
    "url": "https://github.com/adopted-ember-addons/ember-electron/issues"
  },
  "scripts": {
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "test": "yarn-or-npm run test-node-all && yarn-or-npm run lint",
    "test-fast": "yarn-or-npm run test-node && yarn-or-npm run lint",
    "test-node": "mocha node-tests/unit/**/*.js ./node-tests/integration/**/*.js",
    "test-node-slow": "mocha ./node-tests/acceptance/**/*.js",
    "test-node-all": "yarn-or-npm run test-node && yarn-or-npm run test-node-slow",
    "lint": "eslint ./*.js app lib node-tests"
  },
  "dependencies": {
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^3.0.2",
    "broccoli-source": "^2.1.1",
    "broccoli-string-replace": "^0.1.2",
    "capture-exit": "^2.0.0",
    "chalk": "^2.4.2",
    "console-ui": "^3.0.4",
    "core-object": "^3.1.5",
    "electron-forge": "~5.2.4",
    "electron-protocol-serve": "^1.3.0",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-version-checker": "^3.1.3",
    "ember-inspector": "3.8.0",
    "execa": "^1.0.0",
    "file-url": "^3.0.0",
    "fs-extra": "^8.0.1",
    "globby": "^9.2.0",
    "lodash": "^4.17.11",
    "quick-temp": "^0.1.8",
    "rimraf": "^2.6.3",
    "rsvp": "^4.8.4",
    "silent-error": "^1.1.1",
    "symlink-or-copy": "^1.2.0",
    "tmp": "0.1.0",
    "tree-kill": "^1.2.1",
    "tree-sync": "^2.0.0",
    "yarn-or-npm": "^2.0.4"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "conventional-changelog-eslint": "^3.0.1",
    "electron-compile": "^6.4.4",
    "electron-prebuilt-compile": "4.0.0",
    "ember-cli": "~3.10.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.1.3",
    "ember-source": "~3.10.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.2.1",
    "eslint": "^5.16.0",
    "eslint-config-ember": "^0.3.0",
    "eslint-plugin-ember": "^6.4.1",
    "eslint-plugin-ember-suave": "^1.0.0",
    "eslint-plugin-node": "^9.1.0",
    "loader.js": "^4.7.0",
    "mocha": "^6.1.4",
    "mock-spawn": "^0.2.6",
    "mockery": "^2.1.0",
    "ncp": "^2.0.0",
    "qunit-dom": "^0.8.5",
    "semantic-release": "^15.13.12",
    "sinon": "^7.3.2",
    "testdouble": "^3.11.0",
    "walk-sync": "^1.1.3"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "defaultBlueprint": "ember-electron",
    "versionCompatibility": {
      "ember": ">=2.3.0"
    },
    "after": [
      "ember-cli-sri",
      "ember-engines"
    ]
  },
  "config": {
    "forge": "./ember-electron/electron-forge-config.js"
  }
}
