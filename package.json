    "test": "tests"
  },
  "scripts": {
    "test": "npm run test-node && ember electron:test",
    "test-node": "mocha ./node-tests/unit/**",
    "lint": "eslint ."
  },
  "repository": "https://github.com/felixrieseberg/ember-electron",
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": {
    "name": "Felix Rieseberg",
